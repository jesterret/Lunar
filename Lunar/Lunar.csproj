<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageVersion>1.5.1</PackageVersion>
        <Authors>Dewera, jesterret</Authors>
        <Description>A lightweight native DLL mapping library that supports mapping directly from memory</Description>
        <RepositoryUrl>https://github.com/jesterret/Lunar</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Lunar.xml</DocumentationFile>
    </PropertyGroup>

</Project>
